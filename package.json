    "@sindresorhus/slugify": "^2.2.1",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-query-devtools": "^5.87.4",
    "axios": "^1.12.2",
    "date-fns": "^4.1.0",
    "firebase": "^11.10.0",
    "firebase-tools": "^13.35.1",
    "generate-emoji-list": "^1.1.0",
    "oidc-react": "^4.0.1",
