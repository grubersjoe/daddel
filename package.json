    "@types/react-helmet": "^6.1.11",
    "@types/react-transition-group": "^4.4.11",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.0.1",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.11.0",
    "history": "^5.3.0",
    "jest": "^29.5.0",
    "jest-localstorage-mock": "^2.4.26",
