{
  "name": "daddel",
  "author": "Jonathan Gruber <gruberjonathan@gmail.com>",
  "version": "0.16.0",
  "private": true,
  "scripts": {
    "build": "tsc && vite build && vite build -c vite-public.config.ts",
    "deploy": "npm run build && firebase deploy --except functions",
    "dev": "concurrently npm:dev:*",
    "dev:vite": "vite",
    "dev:vite-public": "vite build -w -c vite-public.config.ts",
    "dev:emulators": "GOOGLE_APPLICATION_CREDENTIALS=./credentials.json firebase emulators:start --only auth,firestore,functions",
    "format": "prettier --write 'src/**/*' 'functions/src/**/*' './*.{js,json,md}'",
    "lint": "eslint src/ functions/",
    "preview": "vite preview",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.7",
    "@mui/lab": "^5.0.0-alpha.142",
    "@mui/material": "^5.14.7",
    "@mui/system": "^5.14.7",
    "@mui/x-date-pickers": "^6.12.0",
    "@sindresorhus/slugify": "^2.2.1",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/react-query-devtools": "^4.29.12",
    "axios": "^1.5.0",
    "date-fns": "^2.30.0",
    "firebase": "^10.3.0",
    "firebase-tools": "^12.5.2",
    "generate-emoji-list": "^1.1.0",
    "oidc-react": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-helmet-async": "^1.3.0",
    "react-intersection-observer": "^9.4.4",
    "react-router-dom": "^6.11.2",
    "typeface-roboto": "^1.1.13"
  },
  "resolutions": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-typescript": "^7.22.11",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.5.7",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/react-helmet": "^6.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.4",
    "jest-localstorage-mock": "^2.4.26",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "serve": "^14.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.3.9"
  }
}
